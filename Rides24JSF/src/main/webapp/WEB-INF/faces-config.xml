<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
	https://jakarta.ee/xml/ns/jakartaee/web-facesconfig_3_0.xsd"
	version="3.0">
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>mainToCreateRide</from-outcome>
			<to-view-id>/CreateRide.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>mainToQueryRides</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRide.xhtml</display-name>
		<from-view-id>/CreateRide.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createRideToMain</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>queryRidesToMain</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!--
	<converter>
		<description>TipoUsuarioConverter, definido en faces-config.xml</description>
		<converter-id>TipoUsuarioConverter</converter-id>
		<converter-class>modelo.bean.TipoUsuarioConverter</converter-class>
	</converter>
	-->
</faces-config>